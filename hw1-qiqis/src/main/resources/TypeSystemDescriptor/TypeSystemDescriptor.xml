<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>TutorialTypeSystem</name>
    <description>Type System Definition for the tutorial examples -
        as of Exercise 1</description>
    <version>1.0</version>
    <vendor>Apache Software Foundation</vendor>
  <types>
      <typeDescription>
      <name>sentencesTag</name>
      <description>The Type for sentences</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>id</name>
          <description>the id for this sentence
</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>text</name>
          <description>the text for this sentence</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>geneTag</name>
      <description>the type for gene</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    <features>
        <featureDescription>
          <name>id</name>
          <description>the id of sentence for the genetag</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>start</name>
          <description>the start point of the geneTag</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>end</name>
          <description>the end point of geneTag</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>geneName</name>
          <description>the geneName </description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
  </typeSystemDescription>
